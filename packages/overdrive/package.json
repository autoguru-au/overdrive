{
	"name": "@autoguru/overdrive",
	"version": "4.26.4",
	"description": "Overdrive is a product component library, and design system for AutoGuru.",
	"types": "dist/index.d.ts",
	"main": "dist/index.js",
	"keywords": [
		"component",
		"design-system",
		"react",
		"storybook",
		"styleguide",
		"ui",
		"autoguru"
	],
	"homepage": "https://overdrive.autoguru.io",
	"repository": "autoguru-au/overdrive",
	"license": "MIT",
	"sideEffects": [
		"./lib/reset/**/*",
		"./lib/theme/**/*",
		"./lib/themes/**/*",
		"./dist/reset/**/*",
		"./dist/theme/**/*",
		"./dist/themes/**/*"
	],
	"files": [
		"dist"
	],
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"lint": "tsc --noEmit --skipLibCheck",
		"typeEmit": "tsc -d --declarationDir dist --emitDeclarationOnly",
		"build": "babel lib --out-dir dist --extensions '.ts,.tsx, .css'",
		"prepublishOnly": "yarn run build && yarn run typeEmit",
		"watch": "tsc --watch"
	},
	"devDependencies": {
		"@autoguru/icons": "^1.7.28",
		"@autoguru/tsconfig": "1.0.80",
		"@autoguru/utilities": "1.1.1",
		"@popperjs/core": "2.11.6",
		"@types/react": "^18.0.17",
		"@types/react-dom": "^18.0.6",
		"@vanilla-extract/dynamic": "^2.0.3",
		"clsx": "1.2.1",
		"colord": "2.9.3",
		"csstype": "3.1.1",
		"deepmerge": "^4.2.2",
		"eslint-plugin-jest": "26.9.0",
		"intersection-observer": "^0.12.2",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-focus-lock": "2.9.4",
		"react-intersection-observer": "^9.4.3",
		"react-keyed-flatten-children": "1.3.0",
		"react-swipeable": "^7.0.0",
		"webpack": "5.77.0"
	},
	"peerDependencies": {
		"@autoguru/icons": "^1.7.28",
		"@autoguru/utilities": "^1.0.98",
		"@popperjs/core": "^2.10.2",
		"@vanilla-extract/dynamic": "^2.0.3",
		"clsx": "^1.1.1",
		"colord": "^2.0.1",
		"csstype": "^3.0.7",
		"react": ">=16.8",
		"react-dom": ">=16.8",
		"react-focus-lock": "^2.5.0",
		"react-intersection-observer": ">=9.4.3",
		"react-keyed-flatten-children": "^1.3.0",
		"react-swipeable": ">=7.0.0",
		"webpack": "*"
	}
}
