{
	"name": "@autoguru/overdrive",
	"version": "4.5.3",
	"description": "Overdrive is a product component library, and design system for AutoGuru.",
	"types": "dist/index.d.ts",
	"main": "dist/index.js",
	"keywords": [
		"component",
		"design-system",
		"react",
		"storybook",
		"styleguide",
		"ui"
	],
	"homepage": "https://overdrive.autoguru.io",
	"repository": "autoguru-au/overdrive",
	"license": "MIT",
	"sideEffects": [
		"./lib/reset/**/*",
		"./lib/theme/**/*",
		"./lib/themes/**/*",
		"./dist/reset/**/*",
		"./dist/theme/**/*",
		"./dist/themes/**/*"
	],
	"files": [
		"dist"
	],
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"typeCheck": "tsc",
		"typeEmit": "tsc -d --declarationDir dist",
		"build": "babel lib --out-dir dist --extensions '.ts,.tsx, .css' && yarn run typeEmit",
		"prepublishOnly": "yarn run build",
		"watch": "tsc --watch"
	},
	"devDependencies": {
		"@autoguru/icons": "1.7.18",
		"@autoguru/tsconfig": "1.0.79",
		"@autoguru/utilities": "1.1.1",
		"@popperjs/core": "2.11.2",
		"@types/react": "^18.0.9",
		"@types/react-dom": "^18.0.4",
		"clsx": "1.1.1",
		"colord": "2.9.2",
		"csstype": "3.1.0",
		"eslint-plugin-jest": "26.2.2",
		"react": "^18.1.0",
		"react-dom": "^18.1.0",
		"react-focus-lock": "2.8.1",
		"react-keyed-flatten-children": "1.3.0",
		"webpack": "5.69.0"
	},
	"peerDependencies": {
		"@autoguru/icons": "^1.7.4",
		"@autoguru/utilities": "^1.0.98",
		"@popperjs/core": "^2.10.2",
		"clsx": "^1.1.1",
		"colord": "^2.0.1",
		"csstype": "^3.0.7",
		"react": ">=16.8",
		"react-dom": ">=16.8",
		"react-focus-lock": "^2.5.0",
		"react-keyed-flatten-children": "^1.3.0",
		"webpack": "*"
	}
}
