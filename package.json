{
	"name": "overdrive",
	"version": "0.1.0",
	"private": true,
	"license": "MIT",
	"workspaces": [
		"packages/*"
	],
	"scripts": {
		"chromatic": "chromatic test --exit-zero-on-changes --build-script-name storybook:build",
		"format": "prettier --list-different --write \"**/*.+(js|jsx|ts|tsx|json|yml|md|mdx|html)\"",
		"postinstall": "patch-package && manypkg check",
		"lint": "eslint \"packages/**/*.+(ts|tsx)\" --fix --quiet",
		"playroom:build": "playroom build",
		"release": "changeset publish",
		"start": "concurrently -k \"playroom\" \"yarn:storybook\"",
		"storybook": "start-storybook -p 6006",
		"storybook:build": "build-storybook",
		"test": "jest"
	},
	"browserslist": "extends browserslist-config-autoguru",
	"prettier": {
		"bracketSpacing": true,
		"jsxBracketSameLine": true,
		"printWidth": 80,
		"proseWrap": "always",
		"singleQuote": true,
		"tabWidth": 4,
		"trailingComma": "all",
		"useTabs": true
	},
	"resolutions": {
		"react": "17.0.2",
		"react-dom": "17.0.2",
		"webpack": "4.46.0"
	},
	"dependencies": {
		"@autoguru/babel-preset": "1.1.0",
		"@autoguru/eslint-plugin": "1.1.5",
		"@autoguru/jest-preset": "1.0.98",
		"@babel/core": "7.13.15",
		"@babel/runtime-corejs3": "7.13.10",
		"@changesets/cli": "2.16.0",
		"@manypkg/cli": "0.17.0",
		"@octokit/rest": "18.5.5",
		"@storybook/addon-a11y": "6.1.20",
		"@storybook/addon-essentials": "6.1.20",
		"@storybook/addon-knobs": "6.1.20",
		"@storybook/addons": "6.1.20",
		"@storybook/react": "6.1.20",
		"@storybook/theming": "6.1.20",
		"@testing-library/jest-dom": "5.11.9",
		"@testing-library/react": "11.2.7",
		"@testing-library/react-hooks": "5.1.0",
		"@testing-library/user-event": "12.8.0",
		"@types/node": "14.14.31",
		"@types/react": "17.0.2",
		"@types/react-dom": "17.0.1",
		"@types/webpack-env": "1.16.0",
		"babel-loader": "8.2.2",
		"babel-plugin-treat": "1.6.2",
		"browserslist-config-autoguru": "2.0.0",
		"chromatic": "5.7.1",
		"concurrently": "6.0.0",
		"core-js": "3.9.1",
		"csstype": "^3.0.7",
		"eslint": "7.21.0",
		"glob": "7.1.7",
		"jest": "26.6.3",
		"magic-string": "0.25.7",
		"patch-package": "6.4.7",
		"playroom": "0.23.0",
		"plop": "2.7.4",
		"prettier": "2.2.1",
		"react": "17.0.2",
		"react-dom": "17.0.2",
		"react-test-renderer": "17.0.2",
		"rimraf": "3.0.2",
		"style-loader": "1.2.1",
		"treat": "2.0.2",
		"ts-loader": "8.0.17",
		"tslib": "2.1.0",
		"typescript": "4.2.2",
		"webpack": "4.46.0"
	},
	"volta": {
		"node": "15.8.0",
		"yarn": "1.22.10"
	}
}
