{
	"name": "@autoguru/overdrive",
	"version": "2.0.16",
	"description": "Overdrive is a product component library, and design system for AutoGuru.",
	"keywords": [
		"component",
		"design-system",
		"react",
		"storybook",
		"styleguide",
		"ui"
	],
	"homepage": "https://overdrive.autoguru.io",
	"repository": "https://github.com/autoguru-au/overdrive",
	"license": "MIT",
	"sideEffects": [
		"./reset.ts",
		"./themes.ts",
		"./lib/reset/**/*",
		"./lib/theme/**/*",
		"./lib/themes/**/*"
	],
	"main": "./index.ts",
	"scripts": {
		"chromatic": "chromatic test --exit-zero-on-changes --build-script-name storybook:build",
		"format": "prettier --write \"**/*.+(js|jsx|ts|tsx|json|yml|md|mdx|html)\"",
		"lint": "tsc --noEmit && eslint \"lib/**/*.+(ts|tsx)\" --fix --cache --quiet",
		"playroom:build": "playroom build",
		"release": "changeset publish",
		"start": "concurrently -k \"playroom\" \"yarn:storybook\"",
		"storybook": "start-storybook -p 6006",
		"storybook:build": "build-storybook",
		"test": "jest"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"!(stories).{js,tsx,ts,tsx}": [
			"eslint --cache --quiet --fix",
			"jest --bail --findRelatedTests",
			"prettier --write"
		],
		"*.{js,jsx,ts,tsx,json,yml,md,mdx,html}": [
			"prettier --write"
		],
		"package.json": [
			"npx sort-package-json",
			"prettier --write"
		]
	},
	"browserslist": "extends browserslist-config-autoguru",
	"resolutions": {
		"webpack": "4.43.0"
	},
	"dependencies": {
		"@autoguru/icons": "^1.4.4",
		"@autoguru/utilities": "^1.0.97",
		"@popperjs/core": "^2.4.2",
		"clsx": "^1.1.1",
		"react-keyed-flatten-children": "^1.2.0",
		"react-treat": "^1.4.1"
	},
	"devDependencies": {
		"@autoguru/babel-preset": "1.0.97",
		"@autoguru/eslint-plugin": "1.0.85",
		"@autoguru/jest-preset": "1.0.97",
		"@autoguru/tsconfig": "1.0.78",
		"@babel/core": "7.10.3",
		"@babel/runtime-corejs3": "7.10.3",
		"@changesets/cli": "2.9.1",
		"@octokit/rest": "18.0.0",
		"@storybook/addon-a11y": "5.3.19",
		"@storybook/addon-actions": "5.3.19",
		"@storybook/addon-backgrounds": "5.3.19",
		"@storybook/addon-knobs": "5.3.19",
		"@storybook/addon-viewport": "5.3.19",
		"@storybook/react": "5.3.19",
		"@storybook/theming": "5.3.19",
		"@testing-library/jest-dom": "5.10.1",
		"@testing-library/react": "10.3.0",
		"@testing-library/react-hooks": "3.3.0",
		"@types/node": "14.0.13",
		"@types/react": "16.9.38",
		"@types/react-dom": "16.9.8",
		"@types/webpack-env": "1.15.2",
		"babel-loader": "8.1.0",
		"babel-plugin-treat": "1.1.0",
		"browserslist-config-autoguru": "1.0.88",
		"concurrently": "5.2.0",
		"core-js": "3.6.5",
		"csstype": "2.6.10",
		"eslint": "7.3.0",
		"glob": "7.1.6",
		"husky": "4.2.5",
		"jest": "26.1.0",
		"lint-staged": "10.2.11",
		"magic-string": "0.25.7",
		"playroom": "0.21.0",
		"plop": "2.7.1",
		"prettier": "2.0.5",
		"react": "16.13.1",
		"react-dom": "16.13.1",
		"react-test-renderer": "16.13.1",
		"rimraf": "3.0.2",
		"storybook-chromatic": "4.0.2",
		"style-loader": "1.2.1",
		"treat": "1.4.1",
		"ts-loader": "7.0.5",
		"tslib": "2.0.0",
		"typescript": "3.9.5",
		"webpack": "4.43.0"
	},
	"peerDependencies": {
		"react": ">=16.8",
		"react-dom": ">=16.8"
	},
	"volta": {
		"node": "14.5.0",
		"yarn": "1.22.4"
	}
}
