{
  "name": "@autoguru/overdrive",
  "version": "0.0.0-semantically-released",
  "description": "Overdrive is a product component library, and design system for AutoGuru.",
  "keywords": [
    "component",
    "design-system",
    "react",
    "storybook",
    "styleguide",
    "ui"
  ],
  "homepage": "https://overdrive.autoguru.io",
  "repository": "https://github.com/autoguru-au/overdrive",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "sideEffects": [
    "**/*.scss"
  ],
  "main": "./dist/overdrive.cjs.js",
  "module": "./dist/overdrive.esm.js",
  "types": "./dist/components/index.d.ts",
  "scripts": {
    "build": "cross-env NODE_ENV=production node scripts/build.js",
    "chromatic": "chromatic test --exit-zero-on-changes --build-script-name storybook:build",
    "format": "prettier --write \"**/*.+(js|jsx|ts|tsx|css|scss|json|yml|md|mdx|html)\"",
    "generate": "plop",
    "lint": "concurrently \"yarn:lint:ts\" \"yarn:lint:styles\"",
    "lint:styles": "stylelint --ignore-path .gitignore --fix \"lib/**/*.+(scss|css)\"",
    "lint:ts": "tsc --noEmit && eslint \"{*,{lib,config,scripts,.storybook}/**/*}.{js,jsx,ts,tsx}\" --cache --quiet",
    "semantic-release": "semantic-release",
    "start": "yarn run storybook",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "build-storybook",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint --edit --extends @autoguru/commitlint-config",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css,scss,json,yml,md,mdx,html}": [
      "prettier --write",
      "git add"
    ],
    "*.{js,tsx,ts,tsx}": [
      "eslint --cache --quiet --fix",
      "jest --bail --findRelatedTests",
      "prettier --write",
      "git add"
    ],
    "*.{scss,css}": [
      "stylelint --ignore-path .gitignore --fix",
      "prettier --write",
      "git add"
    ],
    "lib/components/**/index.ts": [
      "node scripts/generateReExport",
      "prettier --write",
      "git add"
    ],
    "package.json": [
      "npx sort-package-json",
      "prettier --write",
      "git add"
    ]
  },
  "browserslist": "extends browserslist-config-autoguru",
  "dependencies": {
    "@autoguru/utilities": "^1.0.64",
    "clsx": "^1.0.4"
  },
  "devDependencies": {
    "@autoguru/babel-preset": "^1.0.64",
    "@autoguru/commitlint-config": "^1.0.53",
    "@autoguru/eslint-plugin": "^1.0.62",
    "@autoguru/jest-preset": "^1.0.64",
    "@autoguru/tsconfig": "^1.0.63",
    "@babel/core": "^7.5.5",
    "@babel/runtime-corejs3": "^7.5.5",
    "@commitlint/cli": "^8.1.0",
    "@octokit/rest": "^16.28.5",
    "@semantic-release/commit-analyzer": "^6.3.0",
    "@semantic-release/github": "^5.4.0",
    "@semantic-release/npm": "^5.1.9",
    "@semantic-release/release-notes-generator": "^7.3.0",
    "@storybook/addon-a11y": "^5.1.11",
    "@storybook/addon-actions": "^5.1.11",
    "@storybook/addon-backgrounds": "^5.1.11",
    "@storybook/addon-knobs": "^5.1.11",
    "@storybook/addon-notes": "^5.1.11",
    "@storybook/addon-storysource": "^5.1.11",
    "@storybook/addon-viewport": "^5.1.11",
    "@storybook/components": "^5.1.11",
    "@storybook/core": "^5.1.11",
    "@storybook/react": "^5.1.11",
    "@storybook/theming": "^5.1.11",
    "@testing-library/jest-dom": "^4.0.1",
    "@testing-library/react": "^9.1.2",
    "@types/node": "^12.7.2",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "autoprefixer": "^9.5.1",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.5",
    "browserslist-config-autoguru": "^1.0.59",
    "build-header": "^1.0.2",
    "concurrently": "^4.1.0",
    "copyfiles": "^2.1.1",
    "core-js": "^3.2.1",
    "cosmiconfig": "^5.2.1",
    "cross-env": "^5.2.0",
    "css-loader": "^3.2.0",
    "cssnano": "^4.1.10",
    "eslint": "^6.2.1",
    "fs-extra": "^8.0.1",
    "glob": "^7.1.4",
    "husky": "^3.0.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.7.1",
    "kleur": "^3.0.3",
    "lint-staged": "^9.2.0",
    "node-sass": "^4.11.0",
    "plop": "^2.4.0",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^1.18.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "renovate-config-autoguru": "^1.0.48",
    "resolve-url-loader": "^3.1.0",
    "rimraf": "^3.0.0",
    "rollup": "^1.19.4",
    "rollup-plugin-node-resolve": "^5.0.0",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-typescript2": "^0.22.0",
    "sass-loader": "^7.3.1",
    "semantic-release": "^15.13.21",
    "storybook-chromatic": "^2.1.1",
    "style-loader": "^1.0.0",
    "stylelint": "^10.1.0",
    "stylelint-config-css-modules": "^1.3.0",
    "stylelint-config-prettier": "^5.2.0",
    "stylelint-config-recommended": "^2.1.0",
    "stylelint-config-recommended-scss": "^3.2.0",
    "stylelint-scss": "^3.9.4",
    "tslib": "^1.10.0",
    "typescript": "~3.5.3",
    "webpack": "^4.39.2"
  },
  "peerDependencies": {
    "react": ">=16.8",
    "react-dom": ">=16.8"
  },
  "volta": {
    "node": "12.9.0",
    "yarn": "1.17.3"
  }
}
