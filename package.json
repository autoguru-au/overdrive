{
	"name": "@autoguru/overdrive",
	"version": "4.43.2",
	"description": "Overdrive is a product component library, and design system for AutoGuru.",
	"types": "dist/index.d.ts",
	"main": "dist/index.js",
	"keywords": [
		"component",
		"design-system",
		"react",
		"storybook",
		"styleguide",
		"ui",
		"autoguru"
	],
	"homepage": "https://overdrive.autoguru.io",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/autoguru-au/overdrive.git"
	},
	"license": "MIT",
	"sideEffects": [
		"./lib/reset/**/*",
		"./lib/theme/**/*",
		"./lib/themes/**/*",
		"./dist/reset/**/*",
		"./dist/theme/**/*",
		"./dist/themes/**/*"
	],
	"files": [
		"dist"
	],
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"analyse": "vite build --config vite-analyse.config.ts",
		"build": "babel lib --out-dir dist --extensions '.ts,.tsx,.css' --ignore 'lib/**/*.stories.tsx,lib/stories'",
		"chromatic": "chromatic test --exit-zero-on-changes --build-script-name storybook:build",
		"copy:public": "node scripts/copyPublic.js",
		"check-deps": "npx npm-check-updates@latest --interactive --format group",
		"format": "prettier --list-different --write \"**/*.+(js|jsx|ts|tsx|json|yml|md|mdx|html)\"",
		"format:staged": "prettier --write",
		"prepublishOnly": "yarn dlx rimraf ./dist && yarn run build && yarn run typeEmit",
		"lint:tsc": "tsc --noEmit --skipLibCheck",
		"lint:eslint": "eslint \"lib/**/*.+(ts|tsx)\" --fix --quiet",
		"lint": "yarn run lint:eslint && yarn run lint:tsc",
		"release": "changeset publish",
		"start": "yarn storybook",
		"storybook:extract": "npx sb extract",
		"storybook": "storybook dev -p 6006",
		"storybook:build": "storybook build",
		"test": "vitest --project=unit-tests",
		"test:ci": "yarn vitest run --no-cache --coverage --project=unit-tests",
		"test:a11y": "vitest run --coverage --project=storybook",
		"typeEmit": "tsc -d --declarationDir dist --emitDeclarationOnly",
		"watch": "tsc --watch",
		"prepare": "husky"
	},
	"browserslist": "extends browserslist-config-autoguru",
	"prettier": {
		"bracketSpacing": true,
		"printWidth": 80,
		"proseWrap": "always",
		"singleQuote": true,
		"tabWidth": 4,
		"trailingComma": "all",
		"useTabs": true
	},
	"devDependencies": {
		"@autoguru/babel-preset": "^3.2.0",
		"@autoguru/eslint-plugin": "2.0.0",
		"@autoguru/icons": "1.8.11",
		"@autoguru/tsconfig": "^1.3.1",
		"@autoguru/utilities": "^1.3.3",
		"@babel/cli": "7.27.2",
		"@babel/core": "7.27.1",
		"@babel/plugin-proposal-class-properties": "^7.18.6",
		"@babel/plugin-proposal-export-default-from": "7.27.1",
		"@babel/plugin-proposal-nullish-coalescing-operator": "^7.18.6",
		"@babel/plugin-proposal-object-rest-spread": "^7.20.7",
		"@babel/plugin-proposal-optional-chaining": "^7.21.0",
		"@babel/plugin-syntax-dynamic-import": "^7.8.3",
		"@babel/plugin-transform-classes": "7.27.1",
		"@babel/plugin-transform-runtime": "7.27.1",
		"@babel/plugin-transform-spread": "7.27.1",
		"@babel/plugin-transform-strict-mode": "7.27.1",
		"@babel/preset-env": "7.27.2",
		"@babel/preset-react": "7.27.1",
		"@babel/preset-typescript": "7.27.1",
		"@babel/runtime-corejs3": "7.27.1",
		"@changesets/cli": "2.29.4",
		"@chromatic-com/storybook": "^4.0.0-next.6",
		"@internationalized/date": "^3.8.1",
		"@octokit/rest": "^21.1.1",
		"@popperjs/core": "^2.11.8",
		"@react-stately/toggle": "^3.8.4",
		"@storybook/addon-a11y": "^9.0.0-rc.0",
		"@storybook/addon-docs": "^9.0.0-rc.0",
		"@storybook/addon-links": "^9.0.0-rc.0",
		"@storybook/addon-onboarding": "^9.0.0-rc.0",
		"@storybook/addon-vitest": "9.0.0-rc.0",
		"@storybook/react-vite": "^9.0.0-rc.0",
		"@testing-library/dom": "^10.4.0",
		"@testing-library/jest-dom": "^6.6.3",
		"@testing-library/react": "^16.3.0",
		"@testing-library/react-hooks": "^8.0.1",
		"@testing-library/user-event": "^14.6.1",
		"@types/node": "22.15.19",
		"@types/react": "19.1.4",
		"@types/react-dom": "19.1.5",
		"@types/webpack-env": "^1.18.8",
		"@typescript-eslint/utils": "^8.32.1",
		"@vanilla-extract/babel-plugin": "^1.2.0",
		"@vanilla-extract/css": "^1.17.2",
		"@vanilla-extract/dynamic": "^2.1.3",
		"@vanilla-extract/recipes": "^0.5.5",
		"@vanilla-extract/sprinkles": "^1.6.3",
		"@vanilla-extract/vite-plugin": "^5.0.2",
		"@vanilla-extract/webpack-plugin": "^2.3.19",
		"@vitest/browser": "3.1.4",
		"@vitest/coverage-v8": "3.1.4",
		"@vitest/eslint-plugin": "1.2.0",
		"babel-plugin-add-import-extension": "^1.6.0",
		"babel-plugin-dev-expression": "^0.2.3",
		"babel-plugin-macros": "^3.1.0",
		"browserslist-config-autoguru": "^2.4.4",
		"chromatic": "11.28.2",
		"clsx": "^2.1.1",
		"colord": "^2.9.3",
		"concurrently": "^9.1.2",
		"core-js": "^3.42.0",
		"csstype": "^3.1.3",
		"deepmerge": "^4.3.1",
		"es-toolkit": "1.38.0",
		"eslint": "9.27.0",
		"eslint-plugin-storybook": "9.0.0-rc.0",
		"husky": "^9.1.7",
		"intersection-observer": "^0.12.2",
		"jsdom": "26.1.0",
		"lint-staged": "16.0.0",
		"magic-string": "^0.30.17",
		"mini-css-extract-plugin": "^2.9.2",
		"mockdate": "^3.0.5",
		"playwright": "^1.52.0",
		"plop": "^4.0.1",
		"postcss": "^8.5.3",
		"prettier": "^3.5.3",
		"prop-types": "^15.8.1",
		"rand-seed": "^2.1.7",
		"react": "^19.1.0",
		"react-aria": "^3.40.0",
		"react-aria-components": "^1.9.0",
		"react-dom": "^19.1.0",
		"react-focus-lock": "2.13.6",
		"react-intersection-observer": "^9.16.0",
		"react-keyed-flatten-children": "5.0.0",
		"react-stately": "^3.38.0",
		"react-swipeable": "^7.0.2",
		"rollup-plugin-visualizer": "^5.14.0",
		"storybook": "^9.0.0-rc.0",
		"storybook-addon-tag-badges": "^1.4.0",
		"typescript": "5.8.3",
		"url-loader": "^4.1.1",
		"vite": "6.3.5",
		"vitest": "^3.1.4",
		"webpack": "5.99.8"
	},
	"peerDependencies": {
		"@autoguru/icons": ">=1.8.8",
		"@autoguru/utilities": "^1.3.3",
		"@popperjs/core": "^2.11.8",
		"@vanilla-extract/dynamic": "^2.1.2",
		"@vanilla-extract/recipes": "^0.5.5",
		"clsx": "^2.1.1",
		"colord": "^2.9.3",
		"csstype": "^3.1.3",
		"es-toolkit": "^1.34.1",
		"react": ">=18 || >=19.0.0-rc.0",
		"react-aria": "^3.38.1",
		"react-dom": ">=18 || >=19.0.0-rc.0",
