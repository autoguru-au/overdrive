{
  "name": "@autoguru/overdrive",
  "version": "0.0.0-semantically-released",
  "description": "Overdrive is a product component library, and design system for AutoGuru.",
  "keywords": [
    "component",
    "design-system",
    "react",
    "storybook",
    "styleguide",
    "ui"
  ],
  "homepage": "https://overdrive.autoguru.io",
  "repository": "https://github.com/autoguru-au/overdrive",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "cross-env NODE_ENV=production node scripts/build",
    "chromatic": "chromatic test --exit-zero-on-changes --build-script-name storybook:build",
    "format": "prettier --write \"**/*.+(js|jsx|ts|tsx|css|scss|json|yml|md|mdx|html)\"",
    "lint": "concurrently -r \"yarn:lint:ts\" \"yarn:lint:styles\"",
    "lint:styles": "stylelint --ignore-path .gitignore --fix \"lib/**/*.+(scss|css)\"",
    "lint:ts": "tsc --noEmit && eslint \"{*,{lib,config,scripts,.storybook}/**/*}.{js,jsx,ts,tsx}\" --fix --cache --quiet",
    "semantic-release": "semantic-release",
    "start": "yarn run storybook",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "build-storybook",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint --edit --extends @autoguru/commitlint-config",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css,scss,json,yml,md,mdx,html}": [
      "prettier --write",
      "git add"
    ],
    "*.{js,tsx,ts,tsx}": [
      "eslint --cache --quiet --fix",
      "jest --bail --findRelatedTests",
      "prettier --write",
      "git add"
    ],
    "*.{scss,css}": [
      "stylelint --ignore-path .gitignore --fix",
      "prettier --write",
      "git add"
    ],
    "lib/components/**/index.ts": [
      "node scripts/generateReExport",
      "prettier --write",
      "git add"
    ],
    "package.json": [
      "npx sort-package-json",
      "prettier --write",
      "git add"
    ]
  },
  "browserslist": "extends browserslist-config-autoguru",
  "dependencies": {
    "@autoguru/icons": "^1.3.7",
    "@autoguru/utilities": "^1.0.82",
    "clsx": "^1.0.4"
  },
  "devDependencies": {
    "@autoguru/babel-preset": "^1.0.85",
    "@autoguru/commitlint-config": "^1.0.54",
    "@autoguru/eslint-plugin": "^1.0.82",
    "@autoguru/jest-preset": "^1.0.85",
    "@autoguru/tsconfig": "^1.0.77",
    "@babel/core": "^7.7.7",
    "@babel/plugin-proposal-optional-chaining": "^7.7.5",
    "@babel/runtime-corejs3": "^7.7.7",
    "@commitlint/cli": "^8.3.4",
    "@octokit/rest": "^16.36.0",
    "@semantic-release/commit-analyzer": "^6.3.3",
    "@semantic-release/github": "^5.5.5",
    "@semantic-release/npm": "^5.3.5",
    "@semantic-release/release-notes-generator": "^7.3.5",
    "@storybook/addon-a11y": "^5.2.8",
    "@storybook/addon-actions": "^5.2.8",
    "@storybook/addon-backgrounds": "^5.2.8",
    "@storybook/addon-knobs": "^5.2.8",
    "@storybook/addon-viewport": "^5.2.8",
    "@storybook/components": "^5.2.8",
    "@storybook/core": "^5.2.8",
    "@storybook/react": "^5.2.8",
    "@storybook/theming": "^5.2.8",
    "@testing-library/jest-dom": "^4.0.1",
    "@testing-library/react": "^9.1.4",
    "@types/node": "^12.12.21",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.2",
    "@types/webpack-env": "^1.14.1",
    "acorn": "^7.1.0",
    "acorn-walk": "^7.0.0",
    "autoprefixer": "^9.6.5",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.6",
    "browserslist-config-autoguru": "^1.0.82",
    "concurrently": "^5.0.2",
    "copyfiles": "^2.1.1",
    "core-js": "^3.6.1",
    "cosmiconfig": "^6.0.0",
    "cross-env": "^6.0.0",
    "css-loader": "^3.4.0",
    "cssnano": "^4.1.10",
    "enhanced-resolve": "^4.1.1",
    "eslint": "^6.8.0",
    "fs-extra": "^8.0.1",
    "glob": "^7.1.4",
    "husky": "^3.1.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "kleur": "^3.0.3",
    "lint-staged": "^9.2.5",
    "magic-string": "^0.25.4",
    "node-sass": "^4.11.0",
    "plop": "^2.5.2",
    "postcss": "^7.0.25",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-loader": "^3.0.0",
    "postcss-modules": "^1.4.1",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^1.18.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "renovate-config-autoguru": "^1.0.84",
    "resolve-url-loader": "^3.1.0",
    "rimraf": "^3.0.0",
    "sass-loader": "^8.0.0",
    "semantic-release": "^15.14.0",
    "storybook-chromatic": "^3.4.1",
    "style-loader": "1.0.2",
    "stylelint": "^12.0.0",
    "stylelint-config-css-modules": "^2.0.0",
    "stylelint-config-prettier": "^8.0.0",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-recommended-scss": "^4.0.0",
    "stylelint-scss": "^3.11.0",
    "tslib": "^1.10.0",
    "typescript": "3.7.4",
    "webpack": "^4.41.4",
    "write": "^2.0.0"
  },
  "peerDependencies": {
    "react": ">=16.8",
    "react-dom": ">=16.8"
  },
  "volta": {
    "node": "12.14.0",
    "yarn": "1.21.1"
  }
}
