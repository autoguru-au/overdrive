    "react-keyed-flatten-children": "1.3.0",
    "react-swipeable": "^7.0.0",
    "react-test-renderer": "^18.2.0",
    "react-types": "0.1.0",
    "rimraf": "3.0.2",
    "storybook": "^8.4.5",
    "style-loader": "1.2.1",
    "ts-loader": "8.4.0",
