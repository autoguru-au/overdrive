{
  "name": "@autoguru/overdrive",
  "version": "0.1.0",
  "homepage": "https://overdrive.autoguru.io",
  "repository": "https://github.com/autoguru-au/overdrive",
  "license": "MIT",
  "files": [
    "lib/components/**/*",
    "lib/theme/**/*"
  ],
  "sideEffects": [
    "lib/theme/**/*"
  ],
  "main": "./lib/components/index.ts",
  "scripts": {
    "build:storybook": "build-storybook",
    "format": "prettier --write \"{*,lib/**/*}.+(js|jsx|ts|tsx|css|scss|json|yml|md|mdx|html)\"",
    "lint": "concurrently \"yarn:lint:ts\" \"yarn:lint:styles\"",
    "lint:styles": "stylelint --ignore-path .gitignore --fix \"packages/**/*.+(scss|css)\"",
    "lint:ts": "tsc --noEmit && tslint --fix --project tsconfig.json",
    "start": "start-storybook -p 6006",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "linters": {
      "package.json": [
        "npx sort-package-json",
        "git add"
      ],
      "*.{js,jsx,ts,tsx,css,scss,json,yml,md,mdx,html}": [
        "prettier --write",
        "git add"
      ],
      "*.{js,tsx,ts,tsx}": [
        "jest --bail --findRelatedTests",
        "git add"
      ],
      "*.{scss,css}": [
        "stylelint --ignore-path .gitignore --fix",
        "prettier --write",
        "git add"
      ]
    },
    "relative": true
  },
  "dependencies": {
    "@autoguru/icons": "^1.0.2",
    "@autoguru/utilities": "1.0.12",
    "clsx": "^1.0.3"
  },
  "devDependencies": {
    "@autoguru/babel-preset": "^1.0.12",
    "@autoguru/jest-preset": "^1.0.12",
    "@autoguru/tsconfig": "^1.0.12",
    "@babel/core": "^7.3.3",
    "@storybook/addon-actions": "^4.1.12",
    "@storybook/addon-knobs": "^4.1.12",
    "@storybook/addon-options": "^4.1.12",
    "@storybook/addon-storysource": "^4.1.12",
    "@storybook/addon-viewport": "^4.1.12",
    "@storybook/components": "^4.1.12",
    "@storybook/react": "^4.1.12",
    "@types/node": "^11.10.4",
    "@types/react": "^16.8.0",
    "@types/react-dom": "^16.8.2",
    "autoprefixer": "^9.4.8",
    "awesome-typescript-loader": "^5.2.0",
    "babel-loader": "^8.0.0",
    "concurrently": "^4.1.0",
    "cosmiconfig": "^5.1.0",
    "css-loader": "^2.1.0",
    "css-mqpacker": "^7.0.0",
    "cssnano": "^4.1.10",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.8.0",
    "enzyme-to-json": "^3.3.0",
    "husky": "^1.3.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.0.0",
    "jest-enzyme": "^7.0.0",
    "jest-junit": "^6.3.0",
    "lerna": "^3.13.0",
    "lint-staged": "^8.1.5",
    "node-sass": "^4.11.0",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.6.0",
    "prettier": "^1.16.4",
    "react": "^16.8.2",
    "react-dom": "^16.8.2",
    "resolve-url-loader": "^3.0.1",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.0",
    "stylelint": "^9.10.0",
    "stylelint-config-css-modules": "^1.3.0",
    "stylelint-config-prettier": "^5.0.0",
    "stylelint-config-recommended": "^2.1.0",
    "stylelint-config-recommended-scss": "^3.2.0",
    "stylelint-scss": "^3.5.4",
    "tslib": "^1.9.3",
    "tslint": "^5.13.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^3.6.0",
    "typescript": "3.4.0-dev.20190207",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3"
  },
  "peerDependencies": {
    "react": "^16.8.2",
    "react-dom": "^16.8.2"
  }
}
