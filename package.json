		"react-keyed-flatten-children": "^1.3.0",
		"react-swipeable": ">=7.0.0",
		"webpack": "*"
	},
	"volta": {
		"node": "20.18.1",
		"yarn": "4.5.3"
	},
	"packageManager": "yarn@4.5.3"
}
