{
	"name": "@autoguru/overdrive",
	"version": "2.0.1",
	"description": "Overdrive is a product component library, and design system for AutoGuru.",
	"keywords": [
		"component",
		"design-system",
		"react",
		"storybook",
		"styleguide",
		"ui"
	],
	"homepage": "https://overdrive.autoguru.io",
	"repository": "https://github.com/autoguru-au/overdrive",
	"license": "MIT",
	"sideEffects": [
		"./reset.ts",
		"./themes.ts",
		"./lib/reset/**/*",
		"./lib/theme/**/*",
		"./lib/themes/**/*"
	],
	"main": "./index.ts",
	"scripts": {
		"chromatic": "chromatic test --exit-zero-on-changes --build-script-name storybook:build",
		"format": "prettier --write \"**/*.+(js|jsx|ts|tsx|json|yml|md|mdx|html)\"",
		"lint": "tsc --noEmit && eslint \"lib/**/*.+(ts|tsx)\" --fix --cache --quiet",
		"release": "changeset publish",
		"start": "concurrently -k \"playroom\" \"yarn:storybook\"",
		"storybook": "start-storybook -p 6006",
		"storybook:build": "build-storybook",
		"test": "jest"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"!(stories).{js,tsx,ts,tsx}": [
			"eslint --cache --quiet --fix",
			"jest --bail --findRelatedTests",
			"prettier --write"
		],
		"*.{js,jsx,ts,tsx,json,yml,md,mdx,html}": [
			"prettier --write"
		],
		"lib/components/**/index.ts": [
			"node scripts/generateReExport",
			"prettier --write"
		],
		"package.json": [
			"npx sort-package-json",
			"prettier --write"
		]
	},
	"browserslist": "extends browserslist-config-autoguru",
	"resolutions": {
		"webpack": "4.42.0"
	},
	"dependencies": {
		"@autoguru/icons": "^1.3.8",
		"@autoguru/utilities": "^1.0.96",
		"clsx": "^1.1.0",
		"react-treat": "^1.2.4"
	},
	"devDependencies": {
		"@autoguru/babel-preset": "1.0.96",
		"@autoguru/eslint-plugin": "1.0.84",
		"@autoguru/jest-preset": "1.0.96",
		"@autoguru/tsconfig": "1.0.77",
		"@babel/core": "7.8.7",
		"@babel/runtime-corejs3": "7.8.7",
		"@changesets/cli": "2.6.1",
		"@octokit/rest": "17.1.0",
		"@storybook/addon-a11y": "5.3.17",
		"@storybook/addon-actions": "5.3.17",
		"@storybook/addon-backgrounds": "5.3.17",
		"@storybook/addon-knobs": "5.3.17",
		"@storybook/addon-viewport": "5.3.17",
		"@storybook/react": "5.3.17",
		"@storybook/theming": "5.3.17",
		"@testing-library/jest-dom": "5.1.1",
		"@testing-library/react": "10.0.1",
		"@testing-library/react-hooks": "3.2.1",
		"@types/node": "12.12.29",
		"@types/react": "16.9.23",
		"@types/react-dom": "16.9.5",
		"@types/webpack-env": "1.15.1",
		"babel-loader": "8.0.6",
		"babel-plugin-treat": "1.1.0",
		"browserslist-config-autoguru": "1.0.87",
		"concurrently": "^5.1.0",
		"core-js": "3.6.4",
		"csstype": "2.6.9",
		"eslint": "6.8.0",
		"glob": "7.1.6",
		"husky": "4.2.3",
		"jest": "25.1.0",
		"lint-staged": "10.0.8",
		"magic-string": "0.25.7",
		"playroom": "^0.15.1",
		"plop": "2.6.0",
		"prettier": "1.19.1",
		"react": "16.13.0",
		"react-dom": "16.13.0",
		"react-test-renderer": "16.13.0",
		"rimraf": "3.0.2",
		"semantic-release": "17.0.4",
		"storybook-chromatic": "3.5.2",
		"style-loader": "1.1.3",
		"treat": "1.2.4",
		"ts-loader": "6.2.1",
		"tslib": "1.11.1",
		"typescript": "3.8.3",
		"webpack": "4.42.0"
	},
	"peerDependencies": {
		"react": ">=16.8",
		"react-dom": ">=16.8"
	},
	"volta": {
		"node": "12.16.1",
		"yarn": "1.22.1"
	}
}
