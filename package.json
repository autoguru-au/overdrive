		"jsdom": "26.1.0",
		"lint-staged": "16.1.4",
		"magic-string": "0.30.17",
		"mini-css-extract-plugin": "2.9.3",
		"mockdate": "3.0.5",
		"playwright": "1.53.2",
		"plop": "4.0.1",
		"postcss": "8.5.6",
		"prettier": "3.6.2",
		"prop-types": "15.8.1",
