{
  "name": "@autoguru/overdrive",
  "version": "4.34.0",
  "description": "Overdrive is a product component library, and design system for AutoGuru.",
  "types": "dist/index.d.ts",
  "main": "dist/index.js",
  "keywords": [
    "component",
    "design-system",
    "react",
    "storybook",
    "styleguide",
    "ui",
    "autoguru"
  ],
  "homepage": "https://overdrive.autoguru.io",
  "repository": "https://github.com/autoguru-au/overdrive",
  "license": "MIT",
  "sideEffects": [
    "./lib/reset/**/*",
    "./lib/theme/**/*",
    "./lib/themes/**/*",
    "./dist/reset/**/*",
    "./dist/theme/**/*",
    "./dist/themes/**/*"
  ],
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "babel lib --out-dir dist --extensions '.ts,.tsx, .css'",
    "chromatic": "chromatic test --exit-zero-on-changes --build-script-name storybook:build",
    "copy:public": "node scripts/copyPublic.js",
    "format": "prettier --list-different --write \"**/*.+(js|jsx|ts|tsx|json|yml|md|mdx|html)\"",
    "postinstall": "patch-package",
    "prepublishOnly": "yarn run build && yarn run typeEmit",
    "lint:tsc": "tsc --noEmit --skipLibCheck",
    "lint:eslint": "eslint \"lib/**/*.+(ts|tsx)\" --fix --quiet",
    "lint": "yarn run lint:eslint && yarn run lint:tsc",
    "release": "changeset publish",
    "start": "concurrently -k \"yarn:storybook\"",
    "storybook:extract": "npx sb extract",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "test": "jest",
    "typeEmit": "tsc -d --declarationDir dist --emitDeclarationOnly",
    "watch": "tsc --watch"
  },
  "browserslist": "extends browserslist-config-autoguru",
  "prettier": {
    "bracketSpacing": true,
    "jsxBracketSameLine": true,
    "printWidth": 80,
    "proseWrap": "always",
    "singleQuote": true,
    "tabWidth": 4,
    "trailingComma": "all",
    "useTabs": true
  },
  "resolutions": {
    "@types/react": "^18.0.9",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "mini-css-extract-plugin": "^2.6.1"
  },
  "devDependencies": {
    "@autoguru/babel-preset": "^1.2.0",
    "@autoguru/eslint-plugin": "1.3.2",
    "@autoguru/icons": "^1.7.28",
    "@autoguru/jest-preset": "1.1.0",
    "@autoguru/tsconfig": "1.0.80",
    "@autoguru/utilities": "1.1.1",
    "@babel/cli": "7.22.5",
    "@babel/core": "7.22.5",
    "@babel/plugin-proposal-class-properties": "^7.16.0",
    "@babel/plugin-proposal-export-default-from": "^7.16.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.16.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.16.0",
    "@babel/plugin-proposal-optional-chaining": "^7.16.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
